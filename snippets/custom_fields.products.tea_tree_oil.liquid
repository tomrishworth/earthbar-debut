{% comment %}
  DO NOT MODIFY THIS FILE DIRECTLY. THIS FILE WILL BE OVERWRITTEN.
  YOU CAN MODIFY THE LIQUID CODE FOR THIS CUSTOM FIELD HERE:
  https://customfields.bonify.io/dashboard/earthbar-nz.myshopify.com/products/configure/153973/edit
{% endcomment %}
{% if product.metafields.custom_fields["tea_tree_oil"] != blank %}
  <div class="custom-field custom-field__tea-tree-oil custom-field__type--checkbox">
    <h3 class="custom-field--title">Tea Tree Oil</h3>
    <div class="custom-field--value">
      {% if product.metafields.custom_fields["tea_tree_oil"] == 1 %}
        TRUE
      {% else %}
        FALSE
      {% endif %}
    </div>
  </div>
{% endif %}